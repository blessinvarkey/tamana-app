                    android:layout_marginTop="13dp"
                    android:layout_marginEnd="53dp"
                    android:layout_marginRight="53dp"
                    tools:src="@tools:sample/avatars" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>
